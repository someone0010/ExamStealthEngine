{
	"name": "StealthEngine",
	"version": "1.0",
	"description": "A set of very stealthy, experiemental cheats for online exam/test/quiz platforms",
	"manifest_version": 2,
	"background": {
		"scripts": [
			"Background.js"
		],
		"persistent": false
	},
	"permissions": [
		"contentSettings",
		"storage"
	],
	"content_scripts": [{
			"matches": [
				"*://*/*"
			],
			"css": [

			],
			"js": [
				"Modules/NoBlur.js",
				"Modules/ForceCopyPasteCut.js"
			],
			"run_at": "document_start"
		},
		{
			"matches": [
				"*://*.testportal.pl/*",
				"*://*.testportal.net/*"
			],
			"css": [

			],
			"js": [
				"Modules/NoIntervals.js"
			],
			"run_at": "document_start"
		},
		{
			"matches": [
				"*://*.brainly.pl/*",
				"*://*.brainly.com/*"
			],
			"css": [

			],
			"js": [
				"Modules/NoLocalStorage.js"
			],
			"run_at": "document_start"
		}
	]
}